NODEA_KRT_IS_LAST_NODE=false

# NODE B
NODEB_KRT_NODE_NAME=nodeB
NODEB_KRT_NATS_INPUT=runtime1-greeter-v1-Greet.node-b
NODEB_KRT_NATS_OUTPUT=runtime1-greeter-v1-Greet.node-c
NODEB_KRT_NATS_ENTRYPOINT_SUBJECT=runtime1-greeter-v1-Greet.entrypoint
NODEB_KRT_HANDLER_PATH=src/node/node_handler.py
NODEB_KRT_IS_LAST_NODE=false

# NODE C
NODEC_KRT_NODE_NAME=nodeC
NODEC_KRT_NATS_INPUT=runtime1-greeter-v1-Greet.node-c
